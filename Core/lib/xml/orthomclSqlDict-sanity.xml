<gbrowseSanity>
    <sqlValue name="aa_seq_id_PF11_0344">
      <sql>
      select aa_sequence_id from dots.externalaasequence where source_id = 'PF11_0344'
      </sql>
    </sqlValue>

    <sqlValue name="aa_sequence_id">
      <sql>
      <![CDATA[
      select aa_sequence_id
      from apidb.OrthologGroupAaSequence
      where rownum <= ROWNUM_COUNT
      ]]>
      </sql>
    </sqlValue>

    <sqlValue name="db_ref_id">
      <sql>
      <![CDATA[
      select db_ref_id
      from sres.dbref
      where rownum <= ROWNUM_COUNT
      ]]>
      </sql>
    </sqlValue>

    <sqlValue name="ortholog_group_id">
      <sql>
      <![CDATA[
      select ortholog_group_id
      from apidb.orthologgroup
      where rownum <= ROWNUM_COUNT
      ]]>
      </sql>
    </sqlValue>

    <sqlValue name="ortholog_group_name">
      <sql>
      <![CDATA[
      select name
      from apidb.orthologgroup
      where rownum <= ROWNUM_COUNT
      ]]>
      </sql>
    </sqlValue>

    <sqlValue name="sequence_source_id">
      <sql>
      <![CDATA[
      select source_id
      from apidb.orthologgroupaasequence gs, dots.externalaasequence s
      where gs.aa_sequence_id = s.aa_sequence_id 
      and rownum <= ROWNUM_COUNT
      ]]>
      </sql>
    </sqlValue>

    <sqlValue name="taxon_id">
      <sql>
      <![CDATA[
      select taxon_id
      from apidb.orthomcltaxon
      where rownum <= ROWNUM_COUNT
      ]]>
      </sql>
    </sqlValue>

